   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "26f312d4-c087-49e6-9448-87e3617593e5",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "123456789012 is not a valid EAN-13\n"
     ]
    }
   ],
   "source": [
    "if is_valid_ean13(ean_input):\n",
    "    print(f'{ean_input} is a valid EAN-13')\n",
    "else:\n",
    "    print(f'{ean_input} is not a valid EAN-13')"
