   "source": [
    "def is_valid_ean13(ean):\n",
    "    return bool(re.match(r'^\\d{13}$', ean))\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "e780ab1b-9bad-4d21-b417-01753bf799df",
   "metadata": {},
